    "elm-review": "^2.10.3",
    "elm-test": "^0.19.1-revision12",
    "elm-ts-interop": "^0.0.8",
    "elm-webpack-loader": "^8.0.0",
    "esbuild": "^0.20.0",
    "jest": "^29.7.0",
